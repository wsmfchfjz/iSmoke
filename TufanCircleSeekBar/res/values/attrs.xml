<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TufanCircleSeekBar">
        <attr name="wheel_stroke_width" format="integer" />
        <attr name="pointer_radius" format="integer" />
        <attr name="pointer_halo_radius" format="integer" />
        <attr name="max" format="integer"></attr>
        <attr name="init_progress" format="integer"></attr>
        <attr name="show_text" format="boolean"></attr>
        <attr name="format_to_time" format="boolean"></attr>
        <attr name="angle_start" format="integer"></attr>
        <attr name="angle_end" format="integer"></attr>
        <attr name="text_size" format="integer"></attr>
        <attr name="wheel_active_color" format="color"></attr>
        <attr name="wheel_unactive_color" format="color"></attr>
        <attr name="wheel_second_active_color" format="color"></attr>
        <attr name="pointer_color" format="color"></attr>
        <attr name="pointer_halo_color" format="color"></attr>
        <attr name="text_color" format="color"></attr>
    </declare-styleable>

</resources>
